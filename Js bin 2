var nums = [666, 42, -28736, -44234, 42397, 803486, 3975, 4283786, 45676545676, 467813648716846, 81537167412, 4612876487164, 1647812682, 147361289, 492174917984, 124786128746, 126837, 21874981, 7271, 4232, 1246872142174921794, 127419824798127984714, 4028403280952, 94246812, 78923759823850923824319, 41798214682358, -2184971014, -4714646, 316, 9318, -31277, 391738];

var len = nums.length;

